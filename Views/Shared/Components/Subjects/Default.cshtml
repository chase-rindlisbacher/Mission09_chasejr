@{
}
@model IEnumerable<string>


<div class="d-grid gap-2">
    <a asp-controller="Home" asp-action="Index" asp-route-category="" asp-route-pageNum="1" class="btn btn-secondary">Home</a><br />
    @foreach (string s in Model)
    {
        <a asp-controller="Home" asp-action="Index" asp-route-category="@s" asp-route-pageNum="1" class="btn @(ViewBag.SelectedCategory == s ? "btn-primary" : "btn-outline-secondary")">@s</a><br />
    }
</div>